citHeader("To cite snatools use:")

citEntry(
  entry        = "Manual",
  title        = "{snatools}: Tools for Social Network Analysis",
  author       = as.person("Brendan Knapp"),
  year         = "2019",
  note         = "R package version 0.0.0.9",
  url          = "https://knapply.github.io/snatools",
  key          = "snatools-package",
  textVersion  = paste(
    "Knapp, B. G. (2019). snatools: Tools for Social Network Analysis",
    "R package version 0.0.0.9.",
    "Retrieved from https://knapply.github.io/snatools"
  )
)
